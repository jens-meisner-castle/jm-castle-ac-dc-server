    "@types/luxon": "^3.0.1",
    "@types/nodemailer": "^6.4.6",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "jm-castle-ac-dc-calc": "^0.9.8",
    "jm-castle-ac-dc-types": "^0.12.0",
    "jm-castle-mariadb": "^0.3.0",
    "jm-castle-types": "^0.4.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
