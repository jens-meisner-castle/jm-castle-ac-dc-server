{
  "name": "jm-castle-ac-dc-server",
  "version": "0.13.0",
  "type": "module",
  "main": "dist/index.mjs",
  "private": false,
  "scripts": {
    "prebuild": "eslint \"./src/**\"",
    "build": "tsc --project tsconfig.json",
    "start": "node --experimental-modules ./dist/index.mjs",
    "dev": "nodemon --ext '*.mts' -x \"npm run build && npm run start \"",
    "dev:serve": "cross-env NODE_ENV=development ts-node-dev --inspect=9229 --nolazy --respawn ./src -r ts-node/register src/."
  },
  "peerDependencies": {
    "jm-castle-ac-dc-calc": "^0.x",
    "jm-castle-ac-dc-types": "^0.x",
    "jm-castle-mariadb": "^0.x",
    "jm-castle-types": "^0.x"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "luxon": "^3.3.0",
    "mariadb": "^3.0.1",
    "mathjs": "^11.2.1",
    "mqtt": "^4.3.7",
    "node-fetch": "^3.2.10",
    "nodemailer": "^6.8.0",
    "typescript": "^4.8.3",
    "ws": "^8.10.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "4.17.13",
